{
  "denon": {
    "denonName": "default",
    "denonHost": "blank",
    "denonConnectionType": "telnet"
  },
  "mqtt": {
    "brokerIp": "localhost",
    "brokerPort": 1883,
    "brokerUsername": null,
    "brokerPassword": null,
    "brokerUseTls": false
  },
  "Serilog": {
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "pathPath": "logs/HomeAutio.Mqtt.Denon.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31
        }
      }
    ]
  }
}
